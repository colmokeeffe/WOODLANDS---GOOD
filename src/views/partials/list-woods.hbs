<table class="ui inverted fixed table">
  <thead>
    <tr>
      <th>Wood</th>
      <th>Description</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each category.woods}}
      <tr>
        <td>
          {{title}}
        </td>
        <td>
          {{description}}
        </td>
        <td>
          {{latitude}}
        </td>
        <td>
          {{longitude}}
        </td>
        <td>
          <a href="/category/{{../category._id}}/deletewood/{{_id}}" class="ui right floated mini red button">Delete Woods
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
