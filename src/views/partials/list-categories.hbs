{{#each categories}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
    </h2>
    <a href="/category/{{_id}}" class="ui green button">
        <i class="fas fa-folder-open"></i>
    </a>
    <a href="/dashboard/deletecategory/{{_id}}" class="ui red button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}
